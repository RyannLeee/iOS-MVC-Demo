source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '9.0'
inhibit_all_warnings! # 不引入框架警告
use_frameworks!

def pods
    pod 'MJRefresh', '~> 3.2.0'
    pod 'SDWebImage', '~> 5.1.0'
    pod 'SDAutoLayout', '~> 2.2.1'
    pod 'MBProgressHUD', '~> 1.1.0'
    pod 'RTRootNavigationController', '~> 0.7.1'
end

target 'LEEDemoForMVC' do
    pods
end

